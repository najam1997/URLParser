from bs4 import BeautifulSoup
import os
import optparse
import re

def printLinks(url):
 page = ab.open(url)
 html = page.read()
 
 try:
  print ('Urls From Regex:')
  link_scrape = re.compile('href="(.*?)"')
  urls = link_scrape.findall(html)
  for link in urls:
   print (link)
 except:
  pass
 try:
  print ('Urls From BeautifulSoup:')
  soup = BeautifulSoup(html, 'html.parser')
  urls = soup.findAll(name='a')
  for link in urls:
   if link.has_key('href'):
    print (link['href'])
 except:
  pass
